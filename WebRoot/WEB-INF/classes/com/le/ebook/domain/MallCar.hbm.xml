<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.le.ebook.domain.MallCar" table="t_mallcar">
		<id name="mallcar_id" column="mallcar_id">
			<generator class="native"></generator>
		</id>
		<!-- 
	private Long mallcar_id;
	private Long user_id;
	private String username;
	private Long product_id;
	private String product_name;
	private Integer count;
	private String time;
			 -->
		<property name="user_id" column="user_id"></property>
		<property name="username" column="username"></property>
		<property name="product_id" column="product_id"></property>
		<property name="product_name" column="product_name"></property>
		<property name="count" column="count"></property>
		<property name="time" column="time"></property>
	</class>
</hibernate-mapping>